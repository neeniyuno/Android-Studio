package com.example.exam;

import androidx.appcompat.app.AppCompatActivity;

import android.content.Intent;
import android.os.Bundle;
import android.view.View;

public class MainActivity extends AppCompatActivity {

    @Override
    protected void onCreate(Bundle savedInstanceState) {
        super.onCreate(savedInstanceState);
        setContentView(R.layout.activity_main);
    }

    public void RegisForm (View v)
    {
        Intent i = new Intent (this, RegisForm.class);
        startActivity(i);
    }


    public void WhatsTheColor (View v)
    {
        Intent i = new Intent (this, WhatsTheColor.class);
        startActivity(i);
    }

    public void Calculator (View v)
    {
        Intent i = new Intent (this, Calculator.class);
        startActivity(i);
    }

    public void Counter (View v)
    {
        Intent i = new Intent (this, Counter.class);
        startActivity(i);
    }
}
